{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "SvelteKit gallery site of standalone TikZ images along with their LaTeX source code",
  "homepage": "https://tikz.netlify.app",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "type": "module",
  "version": "1.0.0",
  "bugs": "https://github.com/janosh/tikz/issues",
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "serve": "yarn build && yarn preview",
    "prebuild": "rm -rf ./static/assets",
    "predev": "node src/parseTexFiles.js && yarn symlinkAssets",
    "symlinkAssets": "cd static && ln -fs ../../assets . && cd -",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.310",
    "@svicons/fa-regular": "^0.1.8",
    "@svicons/octicons": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "image-size": "^1.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rehype-katex": "^6.0.2",
    "rehype-stringify": "^9.0.3",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.6.0",
    "svelte-github-corner": "^0.1.0",
    "svelte-multiselect": "^4.0.6",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "^0.5.8",
    "typescript": "^4.6.3",
    "unified": "^10.1.2",
    "vite": "^2.9.1"
  }
}
