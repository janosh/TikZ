{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "SvelteKit gallery site of standalone TikZ images along with their LaTeX source code",
  "homepage": "https://tikz.netlify.app",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "type": "module",
  "version": "1.0.0",
  "bugs": "https://github.com/janosh/tikz/issues",
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "serve": "yarn build && yarn preview",
    "prebuild": "yarn symlinkAssets && yarn predev",
    "predev": "node src/parseTexFiles.js",
    "symlinkAssets": "cd static && ln -fs ../../assets . && cd -",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "^1.0.0-next.291",
    "@svicons/fa-regular": "^0.1.8",
    "@svicons/octicons": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "image-size": "^1.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-bricks": "^0.1.5",
    "svelte-check": "^2.4.5",
    "svelte-github-corner": "^0.1.0",
    "svelte-preprocess": "^4.10.4",
    "svelte2tsx": "^0.5.5",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
